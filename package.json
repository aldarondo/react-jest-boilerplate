  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-eslint": "4.1.3",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "es6-symbol": "3.1.0",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "jest-cli": "^12.1.1",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "15.1.0",
    "style-loader": "^0.12.3",
    "todomvc-app-css": "^2.0.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.12.1"
